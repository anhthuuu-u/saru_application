<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Client_app"
        tools:targetApi="31">
        <activity

            android:name=".TransactionEditAddressActivity"
            android:exported="false" />
        <activity
            android:name=".TransactionCheckoutActivity"
            android:exported="false" />
        <activity
            android:name=".ProfileActivity"
            android:exported="false" />

        <activity
            android:name=".ProfileEditActivity"
            android:exported="true" />

        <activity
            android:name=".OrderListActivity"
            android:exported="true" />

        <activity
            android:name=".OrderDetailActivity"
            android:exported="true" />

        <activity
            android:name=".ProfileUpdateCardActivity"
            android:exported="true" />

        <activity
            android:name=".OrderReviewActivity"
            android:exported="true" />

        <activity
            android:name=".OrderRequestReturnActivity"
            android:exported="true" />

        <activity
            android:name=".OrderCancelActivity"
            android:exported="true" />

        <activity
            android:name=".CustomerSupportActivity"
            android:exported="true" />

        <activity
            android:name=".LanguageSettingActivity"
            android:exported="true" />

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>