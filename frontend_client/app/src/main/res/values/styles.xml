<?xml version="1.0" encoding="utf-8"?>
<resources>
        <style name="CircularShape">
            <item name="cornerFamily">rounded</item>
            <item name="cornerSize">50%</item>
        </style>
    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@xml/slide_in_left</item>
        <item name="android:windowExitAnimation">@xml/slide_out_left</item>
    </style>
    <style name="CustomBottomNavStyle" parent="Widget.Material3.BottomNavigationView">
        <item name="itemIconTint">@color/bottom_nav_item_color</item>
        <item name="itemTextColor">@color/bottom_nav_item_color</item>
        <item name="android:background">@color/white</item>
        <item name="itemTextAppearance">@style/BottomNavTextAppearance</item>
    </style>

    <style name="BottomNavTextAppearance" parent="TextAppearance.AppCompat">
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:textSize">10sp</item>

    </style>
</resources>